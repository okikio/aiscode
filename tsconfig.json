{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "noUncheckedIndexedAccess": true,
    "noImplicitOverride": true,
    "jsxImportSource": "solid-js",
    "paths": {
      "~/*": ["./src/*"]
    },
    "baseUrl": ".",
  },
  "include": ["src/**/*", "db/**/*"],
  "exclude": [
    "laravel/**/*",
  ]
}